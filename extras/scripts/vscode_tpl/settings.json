{
    "C_Cpp.intelliSenseEngineFallback": "Disabled",
    "C_Cpp.default.intelliSenseMode": "gcc-x86",
    "c-cpp-compile-run.cpp-flags": "-Wall -Wextra -std=c++17 -Wno-pragma-once-outside-header -m32",
    "c-cpp-compile-run.c-flags": "-Wall -Wextra -Wno-pragma-once-outside-header -m32",
    "C_Cpp.default.cppStandard": "c++17",
	"C_Cpp.default.cStandard": "c11",
	"C_Cpp.loggingLevel": "Error",
 	"clang.cxxflags": [
			"-std=c++17",
			"-Wno-pragma-once-outside-header",
			"-m32"
		],
		"clang.cflags": [
			"-std=c11",
			"-m32"
		]
}
